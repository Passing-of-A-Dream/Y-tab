{
  "name": "my start page",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "基于react18开发的一款起始页",
  "icons": {
    "16": "icons/start.png",
    "32": "icons/start.png"
  },
  "author": "Passing-of-A-Dream",
  "permissions": [
    "https://*/*",
    "http://*/*",
    "*://cn.bing.com/*",
    "*://google.com/*",
    "bookmarks",
    "storage"
  ],
  "externally_connectable": {
    "matches": ["http://loaclhost:5173/*"]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "Y-起始页",
    "default_icon": {
      "16": "icons/start.png",
      "32": "icons/start.png"
    }
  }
}
