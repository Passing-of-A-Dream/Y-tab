{
  "name": "my start page",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "基于react18开发的一款起始页",
  "icons": {
    "16": "icons/start.png",
    "32": "icons/start.png"
  },
  "author": "Passing-of-A-Dream",
  "permissions": [
    "https://*/*",
    "http://*/*",
    "*://cn.bing.com/*",
    "*://google.com/*",
    "https://www.baidu.com/*",
    "bookmarks",
    "storage",
    "favicon"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "cookies": ["https://www.baidu.com/"],
  "externally_connectable": {
    "matches": ["http://loaclhost:5173/*"]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ],
  "browser_action": {
    "default_title": "Y-起始页",
    "default_icon": {
      "16": "icons/start.png",
      "32": "icons/start.png"
    }
  }
}
